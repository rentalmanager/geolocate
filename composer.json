{
    "name": "rentalmanager/geolocate",
    "type": "library",
    "description": "Geolocate package for easier property geolocate functionality.",
    "keywords": [
        "rentbits",
        "geocode",
        "geolocate"
    ],
    "homepage": "https://github.com/rentalmanager/geocode",
    "license": "proprietary",
    "authors": [
        {
            "name": "Rentbits",
            "email": "support@rentbits.com",
            "homepage": "https://rentbits.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "RentalManager\\Geolocate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RentalManager\\Geolocate\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RentalManager\\Geolocate\\GeolocateServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    }
}
